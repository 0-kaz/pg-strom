   AND a.city in ('目黒区','所沢市', '杉並区','府中市')
   AND b.city in ('鴻巣市','蕨市','葛飾区','海老名市');

SELECT a.pref, a.city, b.pref, b.city, st_distance(a.geom, b.geom)
  FROM giskanto a, giskanto b
 WHERE a.gid <> b.gid
   AND a.city in ('目黒区','所沢市', '杉並区','府中市')
   AND b.city in ('鴻巣市','蕨市','葛飾区','海老名市');
